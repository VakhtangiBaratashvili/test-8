<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="399.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.miniuniproject.Controller">
   <children>
      <TextField fx:id="oldIdField" layoutX="126.0" layoutY="42.0" promptText="ID" />
      <TextField fx:id="newIdField" layoutX="126.0" layoutY="94.0" promptText="New ID" />
      <TextField fx:id="newFirstNameField" layoutX="126.0" layoutY="146.0" promptText="First name" />
      <TextField fx:id="newLastNameField" layoutX="126.0" layoutY="200.0" promptText="Last name" />
      <RadioButton fx:id="newMaleConfirm" layoutX="126.0" layoutY="235.0" mnemonicParsing="false" onAction="#confirmNewMale" text="Male" />
      <RadioButton fx:id="newFemaleConfirm" layoutX="126.0" layoutY="260.0" mnemonicParsing="false" onAction="#confirmNewFemale" text="Female" />
      <ChoiceBox fx:id="newFacultyChoiceBox" layoutX="126.0" layoutY="290.0" prefWidth="150.0" />
      <Button fx:id="updateStudentBtn" layoutX="126.0" layoutY="338.0" mnemonicParsing="false" onAction="#update" prefHeight="26.0" prefWidth="150.0" text="Update" />
   </children>
</AnchorPane>
