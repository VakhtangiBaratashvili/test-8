<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="383.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.miniuniproject.Controller">
   <children>
      <TextField fx:id="idField" layoutX="117.0" layoutY="54.0" promptText="ID" />
      <TextField fx:id="firstNameField" layoutX="119.0" layoutY="106.0" promptText="First name" />
      <TextField fx:id="lastNameField" layoutX="120.0" layoutY="158.0" promptText="Last name" />
      <RadioButton fx:id="maleConfirmBtn" layoutX="119.0" layoutY="200.0" mnemonicParsing="false" onAction="#maleConfirm" text="Male" />
      <RadioButton fx:id="femaleConfirmBtn" layoutX="119.0" layoutY="228.0" mnemonicParsing="false" onAction="#femaleConfirm" text="Female" />
      <ChoiceBox fx:id="facultyChoiceBox" layoutX="120.0" layoutY="272.0" prefWidth="150.0" />
      <Button fx:id="addStudentBtn" layoutX="120.0" layoutY="324.0" mnemonicParsing="false" onAction="#add" prefHeight="26.0" prefWidth="150.0" text="Add" />
   </children>
</AnchorPane>
